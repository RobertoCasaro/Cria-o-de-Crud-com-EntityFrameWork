                @Html.DisplayNameFor(model => model.IdmedNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdpacNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdmedNavigation.Idmed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdpacNavigation.Idpac)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idfic">Edit</a> |
